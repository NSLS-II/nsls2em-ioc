
record(epid, "$(Sys)$(Dev)$(PID)") {
  field(DESC, "EPID feedback")
  field(INP, "$(Sys)$(Dev)$(CV)")
  field(FMOD, "$(MODE)")
  field(FBON, "Off")
  field(SCAN, "Passive")
  field(KP, "0")
  field(KI, "0")
  field(KD, "0")
  field(I, "0")
  field(DRVL, "0")
  field(DRVH, "0")
  field(EGU, "$(OEGU,)")
  field(PREC, "$(PREC,3)")
  # field(FLNK, "$(Sys)$(Dev)Val:OBuf-Calc")
  info(autosaveFields, "DESC PREC SCAN VAL DRVL DRVH FMOD KP KI KD I")
}
